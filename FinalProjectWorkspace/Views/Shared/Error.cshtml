@model IEnumerable<String>

<br />
<img class="w-100 h-auto rounded mx-auto d-block" src="/images/error.png" alt="Error" title="Error" />

<div class="text-center">
    @if (Model != null)
    {
        foreach (String message in Model)
        {
            <p><span class="text-danger">@message</span></p>
        }
    }


    <a asp-action="Index" asp-controller="Home" class="btn btn-outline-success">Back to Safety</a>

</div>